#!/bin/bash

szUrl=$(zenity --password --width=560)
echo $szUrl| sudo -S date

CAN_I_RUN_SUDO=$(sudo -n uptime 2>&1|grep "load"|wc -l)
if [ ${CAN_I_RUN_SUDO} -gt 0 ]
then

ans=$(zenity  --list  --text "Choose services to be installed/configured?" --checklist  --column "Pick" --column "Services" FALSE "Headless Browser" FALSE "Redis Server" FALSE "MySQL server" FALSE "MySQL Workbench" FALSE "Active Messaging Queue Server (ActiveMQ)" FALSE "SSH server" FALSE "Eclipse" FALSE "SOLR Server" FALSE "Firefox Browser" FALSE "Selenium" FALSE "Java Watcher" FALSE "XLSX Watcher" FALSE "Static IP Setup" FALSE "Java jdk" FALSE "Python" --separator=":"); echo $ans
if [ -z "$ans" ] ; then
   echo "No selection"
   exit 1
fi

IFS=":" ; for word in $ans ; do 
   case $word in
    "Headless Browser")  echo $szUrl| sudo -S apt-get update| tee >(zenity --text "Updating ubuntu Repo Library" --progress --auto-close --auto-kill --pulsate)  ; sleep 10 ; echo $szUrl| sudo -S apt-get -y install xvfb | tee >(zenity --text "Installing Headless Phantomjs Browser" --progress --auto-close --auto-kill --pulsate) ;;
	"Redis Server") echo $szUrl| sudo -S apt-get update| tee >(zenity --text "Updating ubuntu Repo Library" --progress --auto-close --auto-kill --pulsate) ; sleep 10 ; echo $szUrl | sudo -S apt-get -y install build-essential| tee >(zenity --text "Installing Essentials" --progress --auto-close --auto-kill --pulsate) ; sleep 10 ; echo $szUrl | sudo -S apt-get -y install tcl8.5| tee >(zenity --text "Installing TCL dependecies" --progress --auto-close --auto-kill --pulsate) ; sleep 5 ; cd /tmp ; wget http://download.redis.io/releases/redis-stable.tar.gz | tee >(zenity --text "Downloading Redis" --progress --auto-close --auto-kill --pulsate) ; sleep 3 ; sudo tar xzf redis-stable.tar.gz -C /opt/redis | tee >(zenity --text "Unzipping redis to /opt" --progress --auto-close --auto-kill --pulsate) ; sleep 3 ; cd /opt/redis/redis-stable ; make ; sleep 3 ; make test ; sleep 3 ; echo $szUrl | sudo -S make install ; sleep 3 ; cd utils ; echo $szUrl | sudo -S ./install_server.sh ; sleep 3 ; echo $szUrl | sudo -S update-rc.d redis_6379 defaults ;;
	"MySQL server") echo $szUrl| sudo -S apt-get -y install mysql-server  | tee >(zenity --text "Downloading MySQL Server" --progress --auto-close --auto-kill --pulsate) ;;
	"MySQL Workbench") echo $szUrl| sudo -S apt-get -y install mysql-workbench | tee >(zenity --text "Downloading MySQL Workbench" --progress --auto-close --auto-kill --pulsate) ;;
	"Active Messaging Queue Server (ActiveMQ)") cd /tmp ; szAMQVersion=$(zenity  --list  --radiolist --text "Default version selected 5.12.0" --column "Pick" --column "Version" FALSE 1.1 FALSE 1.2 FALSE 1.3 FALSE 1.4 FALSE 1.5 FALSE 2.0 FALSE 2.1 FALSE 3.0 FALSE 3.1 FALSE 3.2.1 FALSE 3.2.2 FALSE 3.2 FALSE 4.0.1 FALSE 4.0.2 FALSE 4.0M4 FALSE 4.0RC2 FALSE 4.0 FALSE 4.1.0 FALSE 4.1.1 FALSE 4.1.2 FALSE 5.0.0 FALSE 5.1.0 FALSE 5.10.0 FALSE 5.10.1 FALSE 5.10.2 FALSE 5.11.0 FALSE 5.11.1 FALSE 5.11.2 FALSE 5.11.3 TRUE 5.12.0 FALSE 5.12.1 FALSE 5.12.2 FALSE 5.13.0 FALSE 5.2.0 FALSE 5.3.0 FALSE 5.3.1 FALSE 5.3.2 FALSE 5.4.0 FALSE 5.4.1 FALSE 5.4.2 FALSE 5.4.3 FALSE 5.5.0 FALSE 5.5.1 FALSE 5.6.0 FALSE 5.7.0 FALSE 5.8.0 FALSE 5.9.0 FALSE 5.9.1) ; wget https://archive.apache.org/dist/activemq/$szAMQVersion/apache-activemq-$szAMQVersion-bin.tar.gz | tee >(zenity --text "Downloading ActiveMQ v$szAMQVersion" --progress --auto-close --auto-kill --pulsate)  ; sudo -S tar zxf apache-activemq-$szAMQVersion-bin.tar.gz -C /opt/activemq  | tee >(zenity --text "Installing ActiveMQ to /opt/activeMQ" --progress --auto-close --auto-kill --pulsate) ;;
	"SSH server") echo $szUrl| sudo -S apt-get -y install openssh-server  | tee >(zenity --text "Installing SecureShell Server" --progress --auto-close --auto-kill --pulsate)  ;;
	"Eclipse") zenity --info --text "Copy from smb://192.168.1.21/share" ;;
	"Firefox Browser") echo $szPass | sudo -S apt-get -y remove firefox ; szVersion=$(zenity  --list  --radiolist --text "Default version selected 43.0.1" --column "Pick" --column "Version" FALSE 49 FALSE 49.0b1 FALSE 49.0b10 FALSE 49.0b2 FALSE 49.0b3 FALSE 49.0b4 FALSE 49.0b5 FALSE 49.0b6 FALSE 49.0b7 FALSE 49.0b8 FALSE 49.0b9 FALSE 48.0.1 FALSE 48.0.2 FALSE 48 FALSE 48.0b1 FALSE 48.0b10 FALSE 48.0b2 FALSE 48.0b3 FALSE 48.0b4 FALSE 48.0b5 FALSE 48.0b6 FALSE 48.0b7 FALSE 48.0b9 FALSE 47.0.1 FALSE 47 FALSE 47.0b1 FALSE 47.0b2 FALSE 47.0b3 FALSE 47.0b4 FALSE 47.0b5 FALSE 47.0b6 FALSE 47.0b7 FALSE 47.0b8 FALSE 47.0b9 FALSE 46.0.1 FALSE 46 FALSE 46.0b1 FALSE 46.0b10 FALSE 46.0b11 FALSE 46.0b2 FALSE 46.0b4 FALSE 46.0b5 FALSE 46.0b6 FALSE 46.0b7 FALSE 46.0b8 FALSE 46.0b9 FALSE 45.0.1 FALSE 45.0.1esr FALSE 45.0.2 FALSE 45.0.2esr FALSE 45 FALSE 45.0b1 FALSE 45.0b10 FALSE 45.0b2 FALSE 45.0b3 FALSE 45.0b4 FALSE 45.0b5 FALSE 45.0b6 FALSE 45.0b7 FALSE 45.0b8 FALSE 45.0b9 FALSE 45.0esr FALSE 45.1.0esr FALSE 45.1.1esr FALSE 45.2.0esr FALSE 45.3.0esr FALSE 45.4.0esr FALSE 44.0.1 FALSE 44.0.2 FALSE 44 FALSE 44.0b1 FALSE 44.0b2 FALSE 44.0b4 FALSE 44.0b6 FALSE 44.0b7 FALSE 44.0b8 FALSE 44.0b9 TRUE 43.0.1 FALSE 43.0.2 FALSE 43.0.3 FALSE 43.0.4 FALSE 43 FALSE 43.0b1 FALSE 43.0b2 FALSE 43.0b3 FALSE 43.0b4 FALSE 43.0b5 FALSE 43.0b6 FALSE 43.0b7 FALSE 43.0b8 FALSE 43.0b9 FALSE 42 FALSE 42.0b1 FALSE 42.0b2 FALSE 42.0b3 FALSE 42.0b4 FALSE 42.0b5 FALSE 42.0b6 FALSE 42.0b7 FALSE 42.0b8 FALSE 42.0b9 FALSE 41.0.1 FALSE 41.0.2 FALSE 41 FALSE 41.0b1 FALSE 41.0b2 FALSE 41.0b3 FALSE 41.0b4 FALSE 41.0b5 FALSE 41.0b6 FALSE 41.0b7 FALSE 41.0b8 FALSE 41.0b9 FALSE 40.0.2 FALSE 40.0.3 FALSE 40 FALSE 40.0b1 FALSE 40.0b2 FALSE 40.0b3 FALSE 40.0b4 FALSE 40.0b6 FALSE 40.0b7 FALSE 40.0b8 FALSE 40.0b9 FALSE 39.0.3 FALSE 39 FALSE 39.0b1 FALSE 39.0b2 FALSE 39.0b3 FALSE 39.0b4 FALSE 39.0b5 FALSE 39.0b6 FALSE 39.0b7 FALSE 38.0.1 FALSE 38.0.1esr FALSE 38.0.5 FALSE 38.0.5b1-2 FALSE 38.0.5b1 FALSE 38.0.5b2 FALSE 38.0.5b